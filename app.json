{
  "expo": {
    "name": "chapi",
    "slug": "chapi",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/happy.9b47deb4196bbb6605ca.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/happy.9b47deb4196bbb6605ca.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/happy.9b47deb4196bbb6605ca.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.mbittu00.chapi"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
